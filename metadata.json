{
  "name":          "rji-buildbot",
  "version":       "0.1.0",
  "author":        "rji",
  "summary":       "Puppet module for Buildbot CI.",
  "description":   "Manage a Buildbot CI installation with Puppet.",
  "license":       "Apache License, Version 2.0",
  "source":        "https://github.com/rji/puppet-buildbot",
  "project_page":  "https://github.com/rji/puppet-buildbot",
  "issues_url":    "https://github.com/rji/puppet-buildbot/issues",
  "operatingsystem_support": [
    {
      "operatingsystem":        "RedHat",
      "operatingsystemrelease": [ "6" ]
    },
    {
      "operatingsystem":        "CentOS",
      "operatingsystemrelease": [ "6" ]
    },
    {
      "operatingsystem":        "Debian",
      "operatingsystemrelease": [ "7" ]
    }
  ],
  "requirements": [
    {
      "name":                "pe",
      "version_requirement": ">= 3.2.0"
    },
    {
      "name":                "puppet",
      "version_requirement": "3.x"
    }
  ],
  "dependencies": [
    {
      "name":          "puppetlabs-concat",
      "version_range": ">= 1.0.0"
    },
    {
      "name":          "puppetlabs-stdlib",
      "version_range": ">= 1.0.0"
    }
  ]
}
